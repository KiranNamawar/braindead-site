{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run generate-seo && vite build",
    "build:prod": "NODE_ENV=production npm run generate-seo && vite build",
    "generate-seo": "node scripts/generate-seo-files.js",
    "validate-seo": "node scripts/validate-seo.js",
    "build:analyze": "npm run build && npx vite-bundle-analyzer",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest run --reporter=dot --silent --bail=1",
    "test:verbose": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage --reporter=basic",
    "test:watch": "vitest --watch",
    "test:ci": "vitest run --coverage --reporter=basic",
    "analyze": "npx vite-bundle-analyzer",
    "type-check": "tsc --noEmit",
    "pre-commit": "npm run lint && npm run type-check && npm run test:ci",
    "security-audit": "npm audit --audit-level=moderate",
    "clean": "rm -rf dist node_modules/.vite",
    "start": "npm run preview",
    "deploy": "npm run build && netlify deploy --prod --dir=dist",
    "deploy:preview": "npm run build && netlify deploy --dir=dist"
  },
  "dependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/marked": "^5.0.2",
    "@types/qrcode": "^1.5.5",
    "crypto-js": "^4.2.0",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.344.0",
    "marked": "^16.0.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "terser": "^5.43.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^1.0.4"
  }
}